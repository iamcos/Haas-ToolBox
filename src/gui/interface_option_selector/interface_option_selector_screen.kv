<InterfaceOptionSelectorScreen>:
    MainWindow:
        TitleLabel:
            id: main_label
            text: "Select interface option"
            
        ScrollView:
            id: scroll_view

        BoxLayout:
            size_hint_y: 0.2
            Button:
                text: 'Back to interface selection'
                on_press: root.manager.current = "interface_selector"
            Button:
                text: 'Back to menu'
                on_press: root.manager.current = 'menu'

