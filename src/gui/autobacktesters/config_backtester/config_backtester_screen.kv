<ConfigBacktesterScreen>:
    MainWindow:
        TitleLabel:
            text: "Config backtester"

        BoxLayout:
            orientation: "vertical"
            spacing: 50

            LogsLayout:
                id: logs_layout

        BoxLayout:
            size_hint_y: 0.1
            Button:
                text: "Change config"
            Button:
                text: "Start"
                on_release: root.start_backtesting()
            Button:
                text: "To menu"

